  <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: 37.7749, lng: -122.4194};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 16,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBAO5uKe5O0cPb0C4ryfvA5vMGVpLARcVA&callback=initMap"></script>

  
<div class = "well">
